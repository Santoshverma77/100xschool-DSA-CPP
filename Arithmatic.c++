#include <iostream>
using namespace std;
// Arithmatic Operators:
//+ - * / % ++ --

int main() {
    int a = 10;
    int b = 3;
    cout << a + b << endl;
    cout << a - b << endl;
    cout << a * b << endl;
    cout << a / b << endl;
    cout << a % b << endl;
    cout << a++ << endl;
    cout << a-- << endl;
    cout << ++a << endl;
    cout << --a << endl;
    return 0;
}

